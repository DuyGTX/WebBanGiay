                                    <i class="fa fa-star-o"></i>
                                    <i class="fa fa-star-o"></i>
                                    <i class="fa fa-star-o"></i>
                                    <i class="fa fa-star-o"></i>
                                </div>
                                <h5>@(shoe.Price?.ToString("N0") ?? "N/A")đ</h5>


                            </div>
                        </div>
                    </div>
                }
