                                    <li>
                                        <a href="#"><img src="~/template/img/icon/compare.png" alt=""> <span>Compare</span></a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="ShopDetail" asp-route-id="@shoe.ShoeId"><img src="~/template/img/icon/search.png" alt=""></a>

                                    </li>
                                </ul>
                            </div>
                            <div class="product__item__text">
                                <h6 asp-controller="Product" asp-action="ViewDetail" asp-route-id="@shoe.ShoeId">@shoe.ShoeName</h6>
